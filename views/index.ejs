<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Posts</title>
</head>
<body>
    <h1>Posts</h1>
    <% posts.forEach(function(post) { %>
        <div>
            <h2><%= post.title %></h2>
            <p><%= post.content %></p>
            <small><%= new Date(post.createdAt).toLocaleDateString() %></small>
        </div>
    <% }) %>
</body>
</html>
